{
  "manifest_version": 2,

  "name": "TwitChat",
  "description": "twitch chat on other Pages",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "permissions": [
   "alarms", "tabs", "bookmarks", "*://*/*"
   ]
}
